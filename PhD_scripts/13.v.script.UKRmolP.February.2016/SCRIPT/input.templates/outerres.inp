&swintfin                 ! used by SW_INTERF (see docs/swinterf)
  name = '>>>MOLECULE<<< - >>>SPIN<<< >>>SYMMETRY<<<',
  mgvn = >>>MGVN<<<,               ! symmetry, 0 = A1, as in CONGEN
  stot = >>>STOT<<<,               ! spin multiplicity 2*S+1
  ntarg = >>>NTARG<<<,              ! number of target states, max. 16
  idtarg = >>>IDTARG<<<              ! mapping of target states from order by target congen inputs to energy order as in unit LUTARG = 24 (output of denprop)
  rmatr = >>>RMATR<<<,           ! R-matrix radius
  ismax = >>>ISMAX<<<,              ! maximum multipole to be retained in expansion of long range potentials
!  lcont = >>>LCONT<<<,			!says how many rows of the eigenvector matrix have been saved on fort.25
  ukrmolp_ints = >>>UKRMOLP<<<, !true if you run UKRmol+ suite, false for old version of code
&end
&rslvin                   ! used by R_SOLVE (see docs/rsolve)
  name = '>>>MOLECULE<<< - >>>SPIN<<< >>>SYMMETRY<<< - K-matrices',
  mgvn = >>>MGVN<<<,               ! symmetry
  stot = >>>STOT<<<,               ! spin multiplicity 2*S+1
  newbut = 0,             ! Buttle correction - 0 in polyatomic codes
  nerang = >>>NERANG<<<,          ! number of energy subranges
  nescat = >>>NESCAT<<<,          ! number of input scattering energies in each subrange
  einc = >>>EINC<<<,     ! scattering energies - initial energy, energy increment, there can be more subranges
  ieunit = >>>IEUNIT<<<,             ! energy unit - 1 = Ryd, 2 = eV
  ikform = 'F',           ! formatted/unformatted switch for unit LUKMT = 19 (K-matrix)
&end
&bprop                    ! read by subroutine ASYM1 called from R_SOLVE (see docs/rprop-cfasym) 
  raf = >>>RAF<<<,             ! radius at which continued fraction method can be used
&end
&phasin                   ! used by EIGEN_P (see docs/eigenp) 
  name = '>>>MOLECULE<<< - >>>SPIN<<< >>>SYMMETRY<<<', 
  ieunit = >>>IEUNIT<<<,             ! energy unit - 1 = Ryd, 2 = eV
  icon = 1,               ! eigenphase smoothing switch - 1 = add multiples of pi
  luphso = 110,           ! formatted eigenphases
  ikform = 'F',           ! formatted/unformatted switch for unit LUKMT = 19 (K-matrix)
&end
&tmatin                   ! used by T_MATRX (see docs/tmatrx) 
  name = '>>>MOLECULE<<< - >>>SPIN<<< >>>SYMMETRY<<<', 
  maxi = >>>MAXI<<<,               ! the highest initial state for which cross sections are required      
  maxf = >>>MAXF<<<,               ! the highest final state for which cross sections are required      
  ikform = 'F',           ! formatted/unformatted switch for unit LUKMT = 19 (K-matrix)
  itform = 'F',		  ! formatted/unformatted switch for unit LUTMT = 12 (T-matrix)

&end
&xsecin                   ! used by I_XSECS (see docs/ixsecs)
  name = '>>>MOLECULE<<< - >>>SPIN<<< >>>SYMMETRY<<<', 
  ieunit = >>>IEUNIT<<<,             ! energy unit - 1 = Ryd, 2 = eV 
  ixsn = >>>IXSN<<<,               ! cross-section unit - 1 = bohr**2, 2 = angstrom**2, 3 = pi*bohr**2
  maxi = >>>MAXI<<<,               ! the highest initial state for which cross sections are required      
  maxf = >>>MAXF<<<,               ! the highest final state for which cross sections are required
  itform = 'F',		  ! formatted/unformatted switch for unit LUTMT = 12 (T-matrix)     
  luxsn = 100,            ! formatted cross sections
&end
&res                      ! used by RESON_F (see docs/reson)
  name = '>>>MOLECULE<<< - >>>SPIN<<< >>>SYMMETRY<<< - Resonance fits'
  lukmt = 19,             ! file with K-matrix
  lures = 27,
  mgvn = >>>MGVN<<<,               ! symmetry
  stot = >>>STOT<<<,               ! spin multiplicity 2*S+1
  nst = 1,                ! label of scattering state symmetry ???
  ikform = 'F',           ! formatted/unformatted switch for unit LUKMT = 19 (K-matrix)
&end
&bprop                    ! read by subroutine ASYM1 called from RESON (see docs/rprop-cfasym) 
  raf = >>>RAF<<<,             ! radius at which continued fraction method can be used
&end
