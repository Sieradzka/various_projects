Cell[CellGroupData[{

Cell["\<\
Rules for converting from SI units to fundamental SI units
Rules for converting from SI units to more compact SI units\
\>", "Subsubsection",
 CellChangeTimes->{{3.604670850877391*^9, 3.6046709258046713`*^9}, {
  3.604670957660076*^9, 3.604670981840231*^9}}],

Cell["\<\
BeginPackage[\"Units`\"];

ToFundRules::usage = \"List of rules to convert SI units to fundamental SI \
units\";
ToConvRules::usage = \"List of rules to convert SI units to simplest \
conventional SI units\";  

ToFundRules = Dispatch[{
\tRadian -> 1, 
\tSteradian -> 1, 
    Newton -> (Kilogram Meter)/Second^2, 
    Pascal -> Kilogram/(Meter Second^2), 
    Joule -> (Kilogram Meter^2)/Second^2, 
    Watt -> (Kilogram Meter^2)/Second^3, Coulomb -> Ampere Second, 
    Volt -> (Kilogram Meter^2)/(Ampere Second^3), 
    Ohm -> (Kilogram Meter^2)/(Ampere^2 Second^3), 
    Siemens -> (Ampere^2 Second^3)/(Kilogram Meter^2), 
    Farad -> (Ampere^2 Second^4)/(Kilogram Meter^2), 
    Weber -> (Kilogram Meter^2)/(Ampere Second^2), 
    Henry -> (Kilogram Meter^2)/(Ampere^2 Second^2), 
    Tesla -> Kilogram/(Ampere Second^2), 
    Lumen -> Candela, 
    Lux -> Candela/Meter^2, 
    Becquerel -> Second^(-1), 
    Hertz -> Second^(-1), 
    GrayDose -> Meter^2/Second^2}
];
 
ToConventionalRules = Dispatch[{
    (* Kilogram Metre/Second^2 -> Newton *)
    (CP_.) Kilogram^(KP_.) Meter^(MP_.) Second^(SP_.) /; 
      IntegerQ[KP] && IntegerQ[MP] && IntegerQ[SP]  ->   
      CP Newton^KP Meter^(MP - KP) Second^(SP + 2 KP),
    
    (* Kilogram/Second^2 -> Newton/Metre *)
    (CP_.) Kilogram^(KP_.) Second^(-2 KP_.) /; 
      IntegerQ[KP] ->   
      CP Newton^KP Meter^(- KP),
    
    (* Newton/(Meter Ampere) -> Tesla *)
    (CP_.) Newton^(KP_.) Meter^(-KP_.) Ampere^(-KP_.) /; 
      IntegerQ[KP]  ->   
      CP Tesla^KP,
     
    (* Newton Meter -> Joule *)
    (CP_.) Newton^(NP_.) Meter^(MP_.) /; 
      IntegerQ[NP] && IntegerQ[MP]  -> 
     CP Joule^NP Meter^(MP - NP), 
    
    (* Joule/Ampere -> Weber *)
    \t(CP_.) Joule^(JP_.) Ampere^(-JP_.) /;
         IntegerQ[JP]  -> 
     \tCP Weber^JP,
    
    (* Joule/Ampere^2 -> Henry *)
    (CP_.) Joule^(JP_.) Ampere^(-2 JP_.) /;
      IntegerQ[JP] -> 
     CP Henry^JP,
    
    (* Henry/Second -> Ohm *)
    (CP_.) Henry^(JP_.) Second^(-JP_.) /;
      IntegerQ[JP] -> 
     CP Ohm^JP,
    
    (* Ampere^2 Second^2/Joule -> Farad *)
    (CP_.) Ampere^(2 AP_.) Second^(2 AP_.) Joule^(-AP_.) /;
      IntegerQ[AP] ->
     CP Farad^AP,
    
    (* Joule/Second -> Watt *)
    (CP_.) Joule^(JP_.) Second^(SP_.) /; 
            IntegerQ[JP] && IntegerQ[SP]  -> 
       CP Watt^JP Second^(SP + JP), 
    
    (* Watt/Ampere -> Volt *)
    ((AP_.) Watt^(NP_.))/Ampere^(NP_.) -> AP Volt^NP, 
    
    (* Watt/Ampere^2 -> Ohm *)
        ((AP_.) Watt^(NP_.))/Ampere^(2 (NP_.)) -> AP Ohm^NP, 
    
    (* Newton/Meter^2 -> Pascal *)
    (CP_.) Newton^(NP_.) Meter^(MP_.) /; 
            IntegerQ[NP] && IntegerQ[MP]  ->  
     CP Pascal^NP Meter^(MP + 2 NP),
    
    (* Ampere Second -> Coulomb *)
    (CP_.) Ampere^(NP_.) Second^(NP_.) -> CP Coulomb^NP
    
    }];
    
ComplexityCount[expn_] := Module[{},
  {Length[Variables[expn]], Apply[Plus, Flatten[Cases[expn, _^(xtemp_.) -> \
Abs[xtemp]]]]}
  ]
  
ToConventional[expn_] := Module[{fundexpn, yy, zz},
  fundexpn = N[SI[expn]] /. ToFundRules;
  yy = FixedPointList[Replace[#, ToConventionalRules] &, fundexpn , Infinity];
  zz = Map[ComplexityCount, yy];
  Extract[yy, First[Position[zz, First[Sort[zz]]]]]
  ]

EndPackage[];\
\>", "Input",
 CellChangeTimes->{{3.604397216771162*^9, 3.6043974007266855`*^9}, {
