Cell[CellGroupData[{

Cell["Example 4\tImplementing contour integration", "Subsubtitle",
 CellChangeTimes->{{3.604345698714932*^9, 3.604345707794148*^9}, {
   3.604604363753127*^9, 3.604604388962771*^9}, 3.6047314928016186`*^9}],

Cell["\<\
ContourInt[expn_, var_, opts___] := Module[
{sols, poles, imptpoles, sgnpoles, posreal, posplus, enclosedpoles, \
residuelist,ans}, 
   sols = Solve[1/expn == 0, var]; 
   poles = Union[var /. sols];
   imptpoles = N[poles] /. Complex[a_,b_] :> b;
   sgnpoles = Simplify[Sign[imptpoles], opts];
   posreal = Position[sgnpoles,0];
   posplus = Position[sgnpoles,1];
   enclosedpoles = Extract[poles, posplus];
   residuelist = Map[Residue[expn, {var, #}] &, enclosedpoles];
   ans = 2 Pi I Apply[Plus, residuelist]; 
   {ans, poles}
]\
\>", "Input",
 CellChangeTimes->{{3.604345742504209*^9, 3.604346067265579*^9}, {
